{
    "name": "frontend",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "dev": "astro dev",
        "start": "astro dev",
        "build": "astro check && pnpm _build",
        "_build": "astro build && renamer --find html --replace html.tera ./dist/**",
        "preview": "astro preview",
        "astro": "astro",
        "format": "prettier --write .",
        "watch": "pnpm _build && chokidar \"**/*.astro\" \"**/*.mjs\" \"public/**\" -i \"node_modules\" -i \"dist\" -c \"pnpm _build\""
    },
    "dependencies": {
        "@astrojs/check": "^0.5.6",
        "astro": "^4.4.15",
        "chokidar-cli": "^3.0.0",
        "prettier": "^3.2.5",
        "renamer": "^5.0.0",
        "typescript": "^5.4.2"
    }
}
